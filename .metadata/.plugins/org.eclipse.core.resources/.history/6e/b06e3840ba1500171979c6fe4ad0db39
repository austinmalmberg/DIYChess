import java.util.Scanner;


public class Calculator {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int first = Integer.parseInt(get(sc, "Enter a number: "));
		int second = get(sc, "Enter another number: ");
		
		System.out.println(first+second);
	}
	
	public static String get(Scanner sc, String prompt, String regex) {
		String s;
		do {
			System.out.print(prompt);
			s = sc.nextLine().trim();
			try {
				return Integer.parseInt(s);
			} catch (Exception e) {}
		} while(true);
	}
}
